+b.table
	+e.row_title
		-titleList = ['Company', 'Covering', 'type', 'Metal level', 'DRugs', 'Primary Care Visits', 'Yearly Deductible', 'Price', null]
		each el, index in titleList
			+e.td(class= `_${tdClassList[index]}`)= el
	each el, index in contentList
		+e.row
			+e.td(class= `_${tdClassList[0]}`)
				+e.td-company
					+e.td-company-image(href='#!')
						+e.IMG.td-company-img(src=`static/img/icons/${el.company[0]}.svg` alt='image')
					+e.td-company-right
						+e.td-company-title(href='#!')= el.company[1]
						+e.td-company-stars(data-stars= el.company[3])
							+i('star')
							+i('star')
							+i('star')
							+i('star')
							+i('star')
			+e.td(class= `_${tdClassList[1]}`)= el.covering
			+e.td(class= `_${tdClassList[2]}`)
				+e.td-text= el.type
				+e.td-helper_info
					+e.IMG.td-helper-img(src='static/img/icons/info.svg' alt='image')
			+e.td(class= `_${tdClassList[3]}`)
				+e.td-helper_color(class= `_${el.level[0]}`)
				+e.td-text= el.level[1]
				+e.td-helper_info
					+e.IMG.td-helper-img(src='static/img/icons/info.svg' alt='image')
			+e.td(class= `_${tdClassList[4]}`)
				+e.td-text= el.drugs
			+e.td(class= `_${tdClassList[5]}`)
				+e.td-text= el.visits
			+e.td(class= `_${tdClassList[6]}`)
				+e.td-text= el.deductible
			+e.td(class= `_${tdClassList[7]}`)
				+e.td-text= el.price + '/mon'
			+e.td(class= `_${tdClassList[8]}`)
				+b.btn_white.__td-btn(href='#!') Overview
				+b.btn_blue.__td-btn(href='#!') Buy